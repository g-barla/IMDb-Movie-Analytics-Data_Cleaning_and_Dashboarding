{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "IMDb_parquet_title_basics",
						"type": "DatasetReference"
					},
					"name": "sourceTitleBasics"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable_title_basics",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_Id as string (\"49\")",
				"}",
				"source(output(",
				"          tconst as string,",
				"          titleType as string,",
				"          primaryTitle as string,",
				"          originalTitle as string,",
				"          isAdult as long,",
				"          startYear as long,",
				"          endYear as long,",
				"          runtimeMinutes as long,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourceTitleBasics",
				"sourceTitleBasics derive(Job_Id = $Job_Id,",
				"          Load_Dt = currentDate()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as decimal(38,0),",
				"          STARTYEAR as decimal(38,0),",
				"          ENDYEAR as decimal(38,0),",
				"          RUNTIMEMINUTES as decimal(38,0),",
				"          GENRES as string,",
				"          JOB_ID as string,",
				"          LOAD_DT as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TCONST = tconst,",
				"          TITLETYPE = titleType,",
				"          PRIMARYTITLE = primaryTitle,",
				"          ORIGINALTITLE = originalTitle,",
				"          ISADULT = isAdult,",
				"          STARTYEAR = startYear,",
				"          ENDYEAR = endYear,",
				"          RUNTIMEMINUTES = runtimeMinutes,",
				"          GENRES = genres,",
				"          JOB_ID = Job_Id,",
				"          LOAD_DT = Load_Dt",
				"     )) ~> sink1"
			]
		}
	}
}